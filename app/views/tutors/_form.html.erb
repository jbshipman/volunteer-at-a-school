<%= form_for @tutor do |f| %>
<%= f.label "Name of Tutor: " %>
<%= f.text_field :name %>
<br>
<br>
<%= f.label "Select a School You Would Like to Volunteer at: " %>
<%= f.collection_select(:school_id, School.all, :id, :name) %>
<br>
<br>
<%= f.label "Enter a New Username: " %>
<%= f.text_field :username %>
<br>
<%= f.submit %>
<% end %>